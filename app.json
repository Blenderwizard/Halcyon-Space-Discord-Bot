{
  "name": "Halycon-Space-Discord-Bot",
  "description": "A Discord Bot developed for the HS Clan Discord Server",
  "repository": "https://github.com/Blenderwizard/Halcyon-Space-Discord-Bot",
  "env": {
    "DB_URI": {
      "description": "The URI Given by mongodb to connect to the database",
      "value": "mongodb+srv://<USERNAME>:<PASSWORD>@<ClusterName>.ju5pb.mongodb.net/hsreactrolls?retryWrites=true&w=majority"
